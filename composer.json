{
    "name": "carsdotcom/laravel-json-schema",
    "type": "library",
    "description": "Json Schema validation for Laravel projects",
    "keywords": [
        "library",
        "jsonschema",
        "laravel",
        "validation"
    ],
    "license": "MIT",
    "require": {
        "laravel/framework": "^9.19 || ^10.0",
        "opis/json-schema": "^2.3",
        "php": "^8.1"
    },
    "require-dev": {
        "brianium/paratest": "^6.6",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "orchestra/testbench": "^7.11",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^9.5.10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Carsdotcom\\JsonSchemaValidation\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Database\\": "database/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Carsdotcom\\JsonSchemaValidation\\SchemaValidatorProvider"
            ],
            "aliases": {
                "SchemaValidator": "Carsdotcom\\JsonSchemaValidation\\SchemaValidator"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}